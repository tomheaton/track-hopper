{
  "manifest_version": 3,
  "name": "Track Hopper",
  "description": "Hop between music streaming services.",
  "version": "1.0.0",
  "options_ui": {
    "page": "options.html"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": ["<all_urls>"]
}
